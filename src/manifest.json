{
  "manifest_version": 3,
  "name": "Unification Web Wallet",
  "homepage_url": "https://unification.com",
  "description": "Unification Web Wallet Browser Extension",
  "default_locale": "en",
  "permissions": [],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Unification Web Wallet",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
